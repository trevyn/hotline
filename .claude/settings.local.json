{
  "permissions": {
    "allow": [
      "Bash(mkdir:*)",
      "Bash(mv:*)",
      "Bash(cargo build:*)",
      "Bash(ls:*)",
      "Bash(rg:*)",
      "Bash(cargo expand:*)",
      "Bash(cargo clean:*)",
      "Bash(rustc:*)",
      "Bash(realpath:*)",
      "Bash(find:*)"
    ],
    "deny": [
      "Bash(cargo run:*)",
      "Bash(./target/release/runtime)",
      "Bash(./target/release/hotline)"
    ]
  },
  "enableAllProjectMcpServers": false
}